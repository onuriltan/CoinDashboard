{
  "name": "@coindashboard/client",
  "version": "0.1.0",
  "description": "Coin Dashboard Client",
  "author": "onuriltan <onur.iltan@gmail.com>",
  "license": "MIT",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test:ci": "CI=true react-scripts test --coverage",
    "test:watch": "react-scripts test --coverage",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --no-error-on-unmatched-pattern --fix"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.9.22",
    "@apollo/react-hooks": "^3.1.5",
    "apollo-boost": "^0.4.7",
    "bootstrap": "^4.5.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "babel-jest-amcharts": "^0.0.2",
    "jest": "24.9.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/serviceWorker.ts",
      "!<rootDir>/src/react-app-env.d.ts",
      "!<rootDir>/src/setupTests.ts"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!(@amcharts)\\/).+\\.(js|jsx|ts|tsx)$"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest-amcharts"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onuriltan/CoinDashboard.git"
  },
  "bugs": {
    "url": "https://github.com/onuriltan/CoinDashboard/issues"
  }
}
